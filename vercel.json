{
  "headers": [
    { "source": "/Build/(.*)\\.gz",        "headers": [{ "key": "Content-Encoding", "value": "gzip" }]},

    { "source": "/Build/*.framework.js.gz","headers": [{ "key": "Content-Type",     "value": "application/javascript" }]},
    { "source": "/Build/*.loader.js.gz",   "headers": [{ "key": "Content-Type",     "value": "application/javascript" }]},
    { "source": "/Build/*.data.gz",        "headers": [{ "key": "Content-Type",     "value": "application/octet-stream" }]},
    { "source": "/Build/*.wasm.gz",        "headers": [{ "key": "Content-Type",     "value": "application/wasm" }]},

    { "source": "/Build/*.js.br",          "headers": [{ "key": "Content-Type",     "value": "application/javascript" }, { "key": "Content-Encoding", "value": "br" }]},
    { "source": "/Build/*.data.br",        "headers": [{ "key": "Content-Type",     "value": "application/octet-stream" }, { "key": "Content-Encoding", "value": "br" }]},
    { "source": "/Build/*.wasm.br",        "headers": [{ "key": "Content-Type",     "value": "application/wasm" }, { "key": "Content-Encoding", "value": "br" }]}
  ]
}
